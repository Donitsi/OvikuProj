<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_settings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.doni.oviku_proj.Settings"
    android:background="@drawable/background_dark">


    <include layout="@layout/toolbartext"
        android:id="@+id/customToolbarSettings"
        />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/settings_list"
        android:layout_below="@+id/customToolbarSettings"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/settings_list"
        style="@style/OvikuButton"
        android:textColor="@color/blueberry"
        android:onClick="RemoveLock"
        android:text="@string/remove_lock"
        android:layout_marginTop="10sp"/>


</RelativeLayout>
