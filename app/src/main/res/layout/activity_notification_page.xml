    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_theme"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.doni.oviku_proj.NotificationPage">


    <include layout="@layout/toolbartext"
        android:id="@+id/customToolbarNotification"
        />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/notification_list"
        android:layout_below="@+id/customToolbarNotification"/>




</RelativeLayout>